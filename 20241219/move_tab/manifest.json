{
  "manifest_version": 3,
  "name": "sequential_num",
  "version": "1.0",
  "description": "商品番号入力欄に[連番]をデフォルトで入力します。",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://world-switch.com/okoku/admin/products/*"
      ]
    }
  ]
}